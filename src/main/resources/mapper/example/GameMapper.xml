<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.feilu.game.box.example.tkmapper.mapper.GameMapper">

    <resultMap id="BaseResultMap" type="com.feilu.game.box.example.tkmapper.entity.Game">
            <id column="id" property="id" jdbcType="INTEGER"></id>
            <result column="category_id" property="categoryId" jdbcType="INTEGER"/>
            <result column="name" property="name" jdbcType="VARCHAR"/>
            <result column="icon" property="icon" jdbcType="VARCHAR"/>
            <result column="picture" property="picture" jdbcType="VARCHAR"/>
            <result column="app_id" property="appId" jdbcType="VARCHAR"/>
            <result column="path" property="path" jdbcType="VARCHAR"/>
            <result column="extra_data" property="extraData" jdbcType="VARCHAR"/>
            <result column="createTime" property="createtime" jdbcType="TIMESTAMP"/>
            <result column="updateTime" property="updatetime" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="BaseColumn">
            id,
            category_id,
            name,
            icon,
            picture,
            app_id,
            path,
            extra_data,
            createTime,
            updateTime
    </sql>

    <sql id="BasicFieldColumn">
            id,
            categoryId,
            name,
            icon,
            picture,
            appId,
            path,
            extraData,
            createtime,
            updatetime
    </sql>

</mapper>
